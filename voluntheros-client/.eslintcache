        Details: {task.details === null ? 'None' : task.details}\n                    {/* {task.date} */}\n                </Card.Text>\n                <Button onClick={(e) => addTask(e, task.id)}>Add to My Tasks</Button> \n            </Card>\n        </>\n    })\n  }\n//if volunteer then gray out card : normal\n//elderly- check to see if elderly_id matches? display : ignore\n  \n\n    function displayTakenTasks() {\n        const takenTasks = tasks.filter(task => task.volunteers_id !== null)\n        return takenTasks.map(task => {\n            console.log(task)\n            return <>\n            <Card className='grid' key={task.id} style={{ width: '18rem' }}>\n            <Card.Header>{task.title}</Card.Header>\n            <Card.Text>\n                Details: {task.details === null ? 'None' : task.details}\n                {/* {task.date} */}\n            </Card.Text>\n            <Button disabled>No Longer Available</Button> \n            </Card>\n            </>\n        })\n    }\n\n    function displayMyTasks() {\n        const myTasks = tasks.filter(task => task.volunteers_id === 4)\n        // setMyAmount(myTasks.length)\n        return myTasks.map(task => {\n            return <>\n            <Card className='grid' key={task.id} style={{ width: '18rem' }}>\n            <Card.Header>{task.title}</Card.Header>\n            <Card.Text>\n                Details: {task.details === null ? 'None' : task.details}\n                {/* {task.date} */}\n            </Card.Text>\n            <Button>Cancel</Button> \n            </Card>\n            </>\n        })\n    }\n\n//id title details volunteers_id elderly_id\n\nfunction addTask(e, key) {\n    console.log(e)\n    // debugger\n    // e.target.parentElement.remove\n\n    updateTasks({id: key, volunteers_id: 4})\n    .then(console.log)\n    // setRefresh(true)\n}\n\n\n\n    return (\n        <div>\n            <div className='container-fluid auto30'>\n            <div className='container'>\n                <h2> Available Tasks</h2>\n                {displayAvailTasks()}\n            </div>\n            <div className='container'>\n                <h2>Claimed Tasks</h2>\n                {displayTakenTasks()}\n            </div>\n            <div className='container'>\n                <h2>My Tasks</h2>\n                {displayMyTasks()}\n            </div>\n            </div>\n        </div>\n    );\n}\n\nexport default Volunteer;",{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","severity":1,"message":"77","line":2,"column":50,"nodeType":"78","messageId":"79","endLine":2,"endColumn":60},{"ruleId":"76","severity":1,"message":"80","line":2,"column":62,"nodeType":"78","messageId":"79","endLine":2,"endColumn":69},{"ruleId":"76","severity":1,"message":"81","line":10,"column":10,"nodeType":"78","messageId":"79","endLine":10,"endColumn":22},{"ruleId":"76","severity":1,"message":"82","line":10,"column":24,"nodeType":"78","messageId":"79","endLine":10,"endColumn":39},{"ruleId":"76","severity":1,"message":"83","line":18,"column":11,"nodeType":"78","messageId":"79","endLine":18,"endColumn":21},{"ruleId":"76","severity":1,"message":"84","line":2,"column":20,"nodeType":"78","messageId":"79","endLine":2,"endColumn":29},{"ruleId":"76","severity":1,"message":"84","line":1,"column":17,"nodeType":"78","messageId":"79","endLine":1,"endColumn":26},{"ruleId":"76","severity":1,"message":"85","line":2,"column":8,"nodeType":"78","messageId":"79","endLine":2,"endColumn":16},{"ruleId":"76","severity":1,"message":"86","line":5,"column":10,"nodeType":"78","messageId":"79","endLine":5,"endColumn":16},{"ruleId":"76","severity":1,"message":"87","line":10,"column":11,"nodeType":"78","messageId":"79","endLine":10,"endColumn":18},{"ruleId":"76","severity":1,"message":"84","line":3,"column":20,"nodeType":"78","messageId":"79","endLine":3,"endColumn":29},{"ruleId":"76","severity":1,"message":"88","line":5,"column":11,"nodeType":"78","messageId":"79","endLine":5,"endCol